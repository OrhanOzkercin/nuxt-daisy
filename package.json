    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vueuse/nuxt": "^8.4.2",
    "c8": "^7.11.2",
    "daisyui": "^2.14.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.7.1",
    "nuxt": "3.0.0-rc.2",
